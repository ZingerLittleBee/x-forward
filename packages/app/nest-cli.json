{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/x-forward-server/src",
  "compilerOptions": {
    "plugins": [
      "@nestjs/swagger"
    ],
    "webpack": true,
    "tsConfigPath": "apps/x-forward-server/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/x-forward-server",
  "projects": {
    "x-forward-server": {
      "type": "application",
      "root": "apps/x-forward-server",
      "entryFile": "main",
      "sourceRoot": "apps/x-forward-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/x-forward-server/tsconfig.app.json"
      }
    },
    "x-forward-client": {
      "type": "application",
      "root": "apps/x-forward-client",
      "entryFile": "main",
      "sourceRoot": "apps/x-forward-client/src",
      "compilerOptions": {
        "tsConfigPath": "apps/x-forward-client/tsconfig.app.json"
      }
    },
    "x-forward-common": {
      "type": "library",
      "root": "libs/x-forward-common",
      "entryFile": "index",
      "sourceRoot": "libs/x-forward-common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/x-forward-common/tsconfig.lib.json"
      }
    }
  }
}