NGINX_BIN=
DOCKER_CONTAINER_NAME=nginx

# 生效的 nginx, 有效值为: NginxBin 或 DockerContainerName
EFFECTED_NGINX=DockerContainerName

# stream 文件存在目录名, 完整路径为: prefix/${STREAM_DIR}
STREAM_DIR=stream

# stream 配置文件名, prefix/${STREAM_DIR}/${STREAM_FILE_NAME}
STREAM_FILE_NAME=stream.conf

# 项目的临时目录文件夹名
TEMP_FILE_NAME=temp

# nginx 主 config 文件名, 完整路径为: prefix/${NGINX_MAIN_CONFIG}
NGINX_MAIN_CONFIG_NAME=nginx.conf

DNS_SERVERS=['4.4.4.4','[2001:4860:4860::8888]','4.4.4.4:1053','[2001:4860:4860::8888]:1053']

# mongo uri
MONGO_URI=mongodb://username:password@127.0.0.1:27017/logs?authSource=admin

# 在线检查间隔, 默认为每五分钟一次
ONLINE_CHECK_CRON='*/5 * * * *'

# client 最大上报秒数
# 如果超过此时间未上报, 则认为客户端已掉线; 反之, 在线
MAXIMUM_REPORTING_SECONDS=300

# Client and Server 通讯密钥
COMMUNICATION_KEY=sdzxcgewqewq

# Client Port, 默认 5000
PORT=5000

# Client 要注册的服务器地址, 默认 http://localhost:3000
SERVER_URL=http://localhost:3000

# Client 外网 IP, 可选, 无默认值
CLIENT_IP=127.0.0.1

# Client Domain, 可选, 无默认值
CLIENT_DOMAIN=xxx.xxx.com
