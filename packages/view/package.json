{
  "name": "@x-forward/view",
  "version": "0.0.1",
  "private": true,
  "description": "ui for nginx stream",
  "scripts": {
    "update-db": "pnpm dlx browserslist@latest --update-db",
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "openapi": "umi openapi",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "1.3.6",
    "@ant-design/icons": "4.7.0",
    "@ant-design/pro-card": "1.20.1",
    "@ant-design/pro-descriptions": "1.10.70",
    "@ant-design/pro-form": "1.66.0",
    "@ant-design/pro-layout": "6.35.0",
    "@ant-design/pro-list": "1.21.57",
    "@ant-design/pro-table": "2.73.0",
    "@umijs/route-utils": "2.0.5",
    "@x-forward/app": "workspace:^0.0.1",
    "@forwardx/shared": "workspace:*",
    "ahooks": "3.3.10",
    "ansi-colors": "4.1.1",
    "antd": "4.20.2",
    "classnames": "2.3.1",
    "figlet": "1.5.2",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21",
    "moment": "2.29.3",
    "omit.js": "2.0.2",
    "rc-resize-observer": "1.2.0",
    "rc-util": "5.21.2",
    "react": "17.0.2",
    "react-dev-inspector": "1.8.0",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.3.0",
    "socket.io-client": "^4.2.0",
    "umi": "3.5.23",
    "umi-serve": "1.9.10",
    "xterm": "4.18.0",
    "xterm-addon-fit": "0.5.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "2.1.5",
    "@types/express": "4.17.13",
    "@types/figlet": "1.5.4",
    "@types/history": "4.7.11",
    "@types/jest": "27.5.0",
    "@types/lodash": "4.14.182",
    "@types/lodash-es": "4.17.6",
    "@types/node": "16.11.33",
    "@types/react": "18.0.9",
    "@types/react-dom": "17.0.16",
    "@types/react-helmet": "6.1.5",
    "@umijs/openapi": "1.4.4",
    "@umijs/plugin-blocks": "2.2.2",
    "@umijs/plugin-esbuild": "1.4.1",
    "@umijs/plugin-openapi": "1.3.2",
    "@umijs/preset-ant-design-pro": "1.3.3",
    "@umijs/preset-dumi": "1.1.40",
    "@umijs/preset-react": "1.8.30",
    "@umijs/yorkie": "2.0.5",
    "autoprefixer": "9.8.8",
    "carlo": "0.9.46",
    "cross-env": "7.0.3",
    "cross-port-killer": "1.4.0",
    "detect-installer": "1.0.2",
    "enzyme": "3.11.0",
    "express": "4.18.1",
    "gh-pages": "3.2.3",
    "jsdom-global": "3.0.2",
    "mockjs": "1.1.0",
    "postcss": "8.4.13",
    "puppeteer-core": "13.7.0",
    "rc-menu": "9.6.0",
    "swagger-ui-react": "4.11.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}