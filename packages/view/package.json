{
  "name": "@x-forward/view",
  "version": "0.0.1",
  "private": true,
  "description": "ui for nginx stream",
  "scripts": {
    "update-db": "pnpm dlx browserslist@latest --update-db",
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "openapi": "umi openapi",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "1.3.6",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-card": "^1.19.0",
    "@ant-design/pro-descriptions": "^1.10.0",
    "@ant-design/pro-form": "^1.64.0",
    "@ant-design/pro-layout": "^6.35.0",
    "@ant-design/pro-list": "1.21.57",
    "@ant-design/pro-table": "^2.71.0",
    "@forwardx/shared": "workspace:*",
    "@umijs/route-utils": "^2.0.0",
    "@x-forward/app": "workspace:^0.0.1",
    "ahooks": "3.3.10",
    "ansi-colors": "4.1.1",
    "antd": "4.20.3",
    "classnames": "2.3.1",
    "figlet": "1.5.2",
    "lodash": "^4.17.0",
    "lodash-es": "4.17.21",
    "moment": "^2.29.0",
    "omit.js": "^2.0.2",
    "rc-menu": "^9.1.0",
    "rc-resize-observer": "1.2.0",
    "rc-util": "^5.16.0",
    "react": "^17.0.0",
    "react-dev-inspector": "^1.7.0",
    "react-dom": "^17.0.0",
    "react-helmet-async": "^1.2.0",
    "socket.io-client": "^4.2.0",
    "swagger-ui-dist": "^4.12.0",
    "umi": "^3.5.0",
    "umi-serve": "1.9.10",
    "xterm": "4.18.0",
    "xterm-addon-fit": "0.5.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.0",
    "@babel/core": "^7.17.10",
    "@playwright/test": "^1.17.0",
    "@types/classnames": "^2.3.1",
    "@types/express": "^4.17.0",
    "@types/figlet": "1.5.4",
    "@types/history": "^4.7.0",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.0",
    "@types/lodash-es": "4.17.6",
    "@types/node": "16.11.33",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@umijs/fabric": "^2.8.0",
    "@umijs/openapi": "^1.6.0",
    "@umijs/plugin-blocks": "^2.2.0",
    "@umijs/plugin-esbuild": "^1.4.0",
    "@umijs/plugin-openapi": "^1.3.3",
    "@umijs/preset-ant-design-pro": "^1.3.0",
    "@umijs/preset-dumi": "^1.1.0",
    "@umijs/preset-react": "^2.1.3",
    "@umijs/yorkie": "2.0.5",
    "autoprefixer": "9.8.8",
    "carlo": "0.9.46",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.0",
    "dva-core": "1.6.0-beta.7",
    "enzyme": "3.11.0",
    "eslint": "^7.32.0",
    "express": "4.18.1",
    "gh-pages": "^3.2.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.0",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.1.0",
    "postcss": "8.4.13",
    "prettier": "^2.5.0",
    "puppeteer-core": "13.7.0",
    "rc-field-form": "^1.26.4",
    "rc-menu": "9.6.0",
    "react-router": "5.0.0",
    "redbox-react": "^1.6.0",
    "redux": "4.x",
    "stylelint": "^13.0.0",
    "swagger-ui-react": "^4.12.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.5.0",
    "umi-serve": "^1.9.10",
    "webpack": "^5.72.0"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
